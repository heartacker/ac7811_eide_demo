{
    "version": 1,
    "beforeBuildTasks": [],
    "afterBuildTasks": [
        {
            "name": "clean",
            "command": "cd \"${OutDir}\" && del *._*"
        }
    ],
    "global": {
        "use-microLIB": false
    },
    "c/cpp-compiler": {
        "optimization": "level-0",
        "language-c": "c99",
        "language-cpp": "c++11",
        "link-time-optimization": true
    },
    "asm-compiler": {},
    "linker": {
        "misc-controls": [
            "--diag_suppress=L6329"
        ]
    }
}